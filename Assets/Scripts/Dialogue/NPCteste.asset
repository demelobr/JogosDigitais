      spanish: 
  - actorName: 
    profile: {fileID: 21300000, guid: 60b55b87fe758e44487fc6e2f80c51fc, type: 3}
    sentence:
      portuguese: "Presos? Mas h\xE1 uma sa\xEDda, certo?"
      english: 
      spanish: 
